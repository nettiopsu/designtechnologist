    "gatsby-transformer-sharp": "^2.3.13",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "tailwindcss": "^1.2.0",
    "typeface-libre-baskerville": "^0.0.72",
    "typeface-roboto": "^0.0.75",
    "typography": "^0.16.19"
